{
  "manifest_version": 3,
  "name": "PluriDocs - SEI!",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "js/libs/jquery-3.6.0.min.js",
        "js/libs/jquery-ui.js",
        "js/libs/papaparse.js",
        "js/injector.js"
      ],
      "css": [
        "css/libs/jquery-ui.css",
        "css/libs/jquery-ui.icon-font.min.css",
        "css/global-styles.css"
      ],
      "all_frames": true,
      "matches": [
        "*://*.br/*controlador.php?acao=arvore_visualizar*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/*",
        "css/*",
        "img/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}